{
	"name": "hdcomputerserver",
	"version": "1.0.0",
	"description": "server for hdcomputer",
	"main": "index.js",
	"scripts": {
		"dev": "NODE_ENV=development nodemon server.ts",
		"start-dev": "ts-node server.ts",
		"start": "node server.js",
		"build": "tsc --project ./",
		"test": "ENV_FILE=.env.test npx jest ./__tests__/ --detectOpenHandles --forceExit --runInBand --passWithNoTests",
		"test:coverage": "ENV_FILE=.env.test npx jest --detectOpenHandles --forceExit --runInBand --passWithNoTests --coverage",
		"db:generate": "npx ts-node models/generateData.ts",
		"db:seed": "npx ts-node models/seed.ts",
		"knex:migrate:make": "knex migrate:make MIGRATE_NAME -x ts",
		"knex:migrate:latest": "knex --knexfile ./knexfile.ts migrate:latest",
		"knex:migrate:rollback": "knex --knexfile ./knexfile.ts migrate:rollback",
		"docker:server": "docker run -p 8084:8084  hd-server:latest",
		"kiot:sync": "ts-node ./thirdparty/syncKiotViet.ts",
		"generate-ctg-prods": "npx ts-node thirdparty/autoGenerateProdCategory.ts"
	},
	"author": "hxtruong",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.21.1",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.0",
		"camelcase-keys": "^6.2.2",
		"chalk": "^4.1.0",
		"class-validator": "^0.13.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-validator": "^6.11.1",
		"fastest-validator": "^1.10.1",
		"jsonwebtoken": "^8.5.1",
		"knex": "^2.0.0",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"multer": "^1.4.2",
		"nanoid": "^3.1.23",
		"pg": "^8.5.1",
		"pg-hstore": "^2.3.3",
		"sharp": "^0.28.1",
		"snakecase-keys": "^3.2.1",
		"ssh2": "^0.8.9",
		"tunnel-ssh": "^4.1.6",
		"uuid": "^8.3.2",
		"xtlog": "^1.0.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.1.1",
		"@commitlint/config-conventional": "^12.1.1",
		"@tsconfig/node14": "^1.0.0",
		"@types/bcrypt": "^3.0.0",
		"@types/cors": "^2.8.10",
		"@types/express": "^4.17.11",
		"@types/jest": "^26.0.22",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/lodash": "^4.14.168",
		"@types/multer": "^1.4.5",
		"@types/node": "^14.14.37",
		"@types/sharp": "^0.28.0",
		"@types/supertest": "^2.0.11",
		"@types/tunnel-ssh": "^4.1.1",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.21.0",
		"@typescript-eslint/parser": "^4.21.0",
		"eslint": "^7.23.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.3.4",
		"jest": "^26.6.3",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.4",
		"ts-node": "^10.8.0",
		"typescript": "^4.2.4"
	}
}
